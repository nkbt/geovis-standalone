{
  "name": "geovis-standalone",
  "productName": "GeoVis",
  "version": "0.0.0",
  "description": "GeoVis Standalone",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron ./app --enable-logging",
    "dev": "NODE_ENV=development npm run start",
    "release": "build --linux --macos --x64",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nkbt/geovis-standalone.git"
  },
  "keywords": [
    "electron-app",
    "electron",
    "geovis"
  ],
  "author": "Nik Butenko <nik@butenko.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkbt/geovis-standalone/issues"
  },
  "homepage": "https://github.com/nkbt/geovis-standalone",
  "devDependencies": {
    "devtron": "1.3.0",
    "electron": "1.3.5",
    "electron-builder": "6.4.1"
  },
  "build": {
    "appId": "org.nkbt.geovis",
    "category": "public.app-category.productivity",
    "npmRebuild": false,
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "target": "deb"
    }
  },
  "directroies": {
    "buildResources": "build",
    "output": "dist",
    "app": "app"
  }
}
